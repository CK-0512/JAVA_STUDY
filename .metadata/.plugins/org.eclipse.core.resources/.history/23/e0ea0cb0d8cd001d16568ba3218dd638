
public class Exam_08 {
	public static void main(String[] args) {
		int loop = 6;
		for (int i = 1; i <= 1; i--) {
			if (i <= 5) {
				loop = loop - 2;
				for (int j = i; j >= i + loop ; j++) {
					System.out.print("*");
				}	
			}
			else {
				for (int j = 1; j <= i; j++) {
					System.out.print("*");
				}
			}
			System.out.println();
		}
	}

}
