package com.jslhrd.map;

import java.util.HashMap;
import java.util.Map;

public class Exam_01 {
	public static void main(String[] args) {
		Map map = new HashMap();
		//저장
		map.put("A", 90);
		map.put("D", 77);
		map.put("C", 95);
		map.put("F", 84);
		map.put("B", 91);
		System.out.println(map);
		map.put("A", 87);
	}
}
